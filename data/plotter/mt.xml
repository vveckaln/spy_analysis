<?xml version="1.0" encoding="ISO-8859-1"?>
<monitor> 


  <!-- W transverse mass distribition (After MT cut)  -->
  <id tag="muCh_adphi_lm" doratio="1" logy="1" cmspublic="y"  oflow="0" uflow="0" title="" ytitle="Events / 5 GeV" xtitle="|#Delta#phi^{lep-met}|"
   yrange="0.1,1000" xrange="0,4" xbin="4" >
    <item dir="PFlow/RecoSteps/lep_tau/OS">Dphi_lepton_met_9</item>
  </id> 

  <id tag="muCh_adphi_tm" doratio="1" logy="1" cmspublic="y"  oflow="0" uflow="0" title="" ytitle="Events / 5 GeV" xtitle="|#Delta#phi^{tau-met}|"
   yrange="0.1,1000" xrange="0,4" xbin="4" >
    <item dir="PFlow/RecoSteps/lep_tau/OS">Dphi_tau_met_9</item>
  </id> 

  <id tag="muCh_dphi_lm" doratio="1" logy="1" cmspublic="y"  oflow="0" uflow="0" title="" ytitle="Events / 5 GeV" xtitle="#Delta#phi^{lep-met}"
   yrange="0.1,10000" xrange="-4,4" xbin="4" >
    <item dir="PFlow/RecoSteps/lep_tau/OS">w_mass_deltaphi_9</item>
  </id> 

  <id tag="muCh_mt_lm" doratio="1" logy="1" cmspublic="y"  oflow="1" uflow="0" title="" ytitle="Events / 5 GeV" xtitle="M^{lep-met}_{T} [GeV]" yrange="0.1,6000"  xbin="16" >
    <item dir="PFlow/RecoSteps/lep_tau/OS">w_mass_9</item>
  </id> 

  <id tag="muCh_mt_tm" doratio="1" logy="1" cmspublic="y"  oflow="1" uflow="0" title="" ytitle="Events / 5 GeV" xtitle="M^{tau-met}_{T} [GeV]" yrange="0.1,6000"  xbin="16" >
    <item dir="PFlow/RecoSteps/lep_tau/OS">w_mass_tau_9</item>
  </id> 

  <id tag="muCh_mt_max" doratio="1" logy="1" cmspublic="y"  oflow="1" uflow="0" title="" ytitle="Events / 5 GeV" xtitle="max(M^{lep-met}_{T},M^{tau-met}_{T}) [GeV]" yrange="0.1,6000" xbin="32" >
    <item dir="PFlow/RecoSteps/lep_tau/OS">w_mass_max_9</item>
  </id> 

  <id tag="muCh_mt_min" doratio="1" logy="1" cmspublic="y"  oflow="1" uflow="0" title="" ytitle="Events / 5 GeV" xtitle="min(M^{lep-met}_{T},M^{tau-met}_{T}) [GeV]" yrange="0.1,6000" xbin="32" xrange="0,300">
    <item dir="PFlow/RecoSteps/lep_tau/OS">w_mass_min_9</item>
  </id> 



</monitor>
